{
	"name": "wikimedia/release-branch",
	"description": "Manage release branches for Wikimedia",
	"type": "project",
	"license": "GPL-3.0-or-later",
	"authors": [
		{
			"name": "Mark A. Hershberger",
			"email": "mah@nichework.com"
		}
	],
	"require": {
		"php": ">=7.0",
		"splitbrain/php-cli": "dev-compact-help",
		"psr/log": "^1.1",
		"hanneskod/classtools": "^1.2",
		"hexmode/php-gerrit": "dev-master",
		"wikimedia/at-ease": "^2.0",
		"christiaan/stream-process": "^1.0",
		"react/event-loop": "^1.1"
	},
	"autoload": {
		"psr-4": {
			"Wikimedia\\Release\\": "src/"
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/hexmode/php-cli"
		}
	],
	"require-dev": {
		"vimeo/psalm": "^3.2",
		"felixfbecker/language-server": "dev-master"
	},
	"minimum-stability": "dev"
}
